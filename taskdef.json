{
	"family": "flask-signup",
	"containerDefinitions": [{
		"name": "flask-signup",
		"image": "049398875557.dkr.ecr.ap-south-1.amazonaws.com/jenkins:%BUILD_NUMBER%",
		"cpu": 10,
		"memory": 256,
		"networkMode": "awsvpc",
		"essential": true,
		"portMappings": [{
			"containerPort": 3000,
			"hostPort": 80
		}]
	}],
	"compatibilities": [
		"EC2",
		"FARGATE"
	],
	"requiresCompatibilities": ["FARGATE"]

}
